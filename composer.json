{
	"name": "silverstripe/deploynaut",
	"type": "silverstripe-module",
	"description": "The SilverStripe deployment framework based on Capistrano",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "capistrano", "deploy"],
	"require": {
		"php": ">=5.4.0",
		"silverstripe/framework": "~3.1",
		"stojg/silverstripe-resque": "1.1.*",
		"gitonomy/gitlib": "dev-master",
		"undefinedoffset/sortablegridfield": "*",
		"silverstripe/secureassets": "dev-master",
		"silverstripe-australia/gridfieldextensions": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~4.8@stable"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.1.x-dev"
		}
	},
	"suggest": {
		"silverstripe/deploynaut-graphite": "Graphite intergration"
	},
	"minimum-stability": "dev"
}
